<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    tools:context = ".activity.MainActivity"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_timeline"
        android:name="com.adgvit.appathon.fragments.timeline"
        android:label="Timeline"
        tools:layout="@layout/fragment_timeline" />
    <fragment
        android:id="@+id/navigation_partners"
        android:name="com.adgvit.appathon.fragments.partners"
        android:label="Partners"
        tools:layout="@layout/fragment_partners" />
    <fragment
        android:id="@+id/navigation_tracks"
        android:name="com.adgvit.appathon.fragments.Tracks"
        android:label="Tracks"
        tools:layout="@layout/fragment_track" />
    <fragment
        android:id="@+id/navigation_faq"
        android:name="com.adgvit.appathon.fragments.faq"
        android:label="FAQ"
        tools:layout="@layout/fragment_faq" />


</navigation>